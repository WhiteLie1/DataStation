.list{
  display: flex;
  flex-wrap: wrap;
  padding: 25rpx 30rpx;
  padding-bottom: 50rpx;
  border-bottom: 2rpx #EAEAEA solid;
}
.listDetail{
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 80rpx;
  padding: 5rpx;
  border: 1rpx solid #EAEAEA;
  box-shadow: 0 0 10rpx #EAEAEA;
  border-radius: 15rpx;
  margin-top: 20rpx;
  background-color: #ffffff;
}

/* 个人项目 */
.bc{
  height: 100%;
  width: 100%;
}
.background {
  width: 100%;
  height: 100%;
  position:fixed; 
  background-size:100% 100%;
  z-index: -1; /*可以让图片置于最底层，不会影响其它部分*/
}
.title{
  display: flex;
  flex-direction: row;
}
.model{
  width:80%;
  display: flex;
  flex-direction: row;
  padding: 25rpx;
  border-bottom: 2rpx #EAEAEA solid;
}
.modelText{
  display:flex;
  flex-direction: column;
}
.left{
  font-size: 35rpx;
  position: relative;
  margin-top: 60rpx;
}
.right{
  font-size: 35rpx;
  position: relative;
  margin-top: 60rpx;
}